pipeline {
  agent any
  stages{
    stage('Checkout Code'){
        steps{
          git 'https://github.com/Janmajey/jenkins_tutorial123'
        }
    }
    stage('Build'){
      steps{
        sh 'echo "Building the applicatio"'
      }
    }
    stage('Test'){
      steps{
        sh 'echo "Running Test"'
      }
    }
    stage('Deploy'){
      steps{
        sh 'echo "Deploying"'
      }
    }
  }
}
